{
  "name": "amarjeet-app",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro build && wrangler dev",
    "astro": "astro",
    "deploy": "astro build && wrangler deploy",
    "typegen": "wrangler types && astro sync",
    "format": "prettier ./**/*.{js,mjs,ts,tsx,astro,mdx,json} -w",
    "prepare": "husky && bun run typegen"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^11.1.0",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/rss": "^4.0.9",
    "@astrojs/sitemap": "^3.2.1",
    "astro": "^4.16.7",
    "astro-breadcrumbs": "^3.2.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "wrangler": "^3.83.0"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,astro,mdx,json}": [
      "prettier -w"
    ],
    "*.astro": [
      "astro check"
    ]
  }
}
